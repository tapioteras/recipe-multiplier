#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules/push-dir/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules/push-dir/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules/push-dir/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules/push-dir/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/push-dir@0.4.1/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/push-dir.js" "$@"
else
  exec node  "$basedir/../../bin/push-dir.js" "$@"
fi
