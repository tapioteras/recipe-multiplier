#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules/parcel/lib/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules/parcel/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules/parcel/lib/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules/parcel/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/parcel@2.12.0_typescript@3.9.10/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/bin.js" "$@"
else
  exec node  "$basedir/../../lib/bin.js" "$@"
fi
