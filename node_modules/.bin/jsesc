#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/jsesc/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/jsesc/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules:/Users/tapioteras/Documents/GitHub/node_modules:/Users/tapioteras/Documents/node_modules:/Users/tapioteras/node_modules:/Users/node_modules:/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/jsesc/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/jsesc/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules:/Users/tapioteras/Documents/GitHub/node_modules:/Users/tapioteras/Documents/node_modules:/Users/tapioteras/node_modules:/Users/node_modules:/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
