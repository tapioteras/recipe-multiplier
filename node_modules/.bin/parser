#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/parser/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/parser/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules:/Users/tapioteras/Documents/GitHub/node_modules:/Users/tapioteras/Documents/node_modules:/Users/tapioteras/node_modules:/Users/node_modules:/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/parser/bin/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/parser/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/@babel/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules:/Users/tapioteras/Documents/GitHub/node_modules:/Users/tapioteras/Documents/node_modules:/Users/tapioteras/node_modules:/Users/node_modules:/node_modules:/Users/tapioteras/Documents/GitHub/recipe-multiplier/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
