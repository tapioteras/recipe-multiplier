    "build": "parcel build index.html --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --force --cleanup --verbose",
    "test": "jest",
    "deliver": "yarn prebuild && yarn build && yarn push-gh-pages"
  },
