    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel": "^2.9.3",
    "prettier": "^2.0.5",
    "process": "^0.11.10",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@babel/preset-env": "^7.22.7",
